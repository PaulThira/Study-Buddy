<Window x:Class="StudyBuddy.View.ManageUsers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudyBuddy.View"
        mc:Ignorable="d"
        Title="ManageUsers" Height="450" Width="800">
    <Grid Background="#FFB19FFC">
        <DataGrid HorizontalAlignment="Center" Height="257" VerticalAlignment="Top" Width="510" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,72,0,0" />
        <TextBlock HorizontalAlignment="Left" Margin="265,12,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="45" Width="240" FontFamily="Lucida Handwriting" TextAlignment="Center" Foreground="#FF5A0B80" FontSize="40"><Run Text="Users"/><LineBreak/><Run/><LineBreak/><Run/></TextBlock>
        <TextBox HorizontalAlignment="Left" Height="62" Margin="145,349,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="185" Background="#FFE7DB5F" Foreground="#FF6753DA" FontFamily="Lucida Handwriting" FontSize="24"/>
        <Button Content="Delete" HorizontalAlignment="Left" Margin="455,349,0,0" VerticalAlignment="Top" Height="62" Width="145" Background="#FFEDE456" Foreground="#FF7549E4" FontFamily="Lucida Handwriting" FontSize="30"/>

    </Grid>
</Window>
